{
    "name": "permittering",
    "version": "0.1.0",
    "private": true,
    "homepage": "/permittering",
    "dependencies": {
        "@amplitude/analytics-browser": "2.11.8",
        "@navikt/aksel-icons": "6.16.0",
        "@navikt/ds-css": "6.16.0",
        "@navikt/ds-react": "6.16.0",
        "@navikt/fnrvalidator": "^1.3.0",
        "@navikt/nav-dekoratoren-moduler": "3.1.1",
        "@navikt/virksomhetsvelger": "0.0.17",
        "@types/react": "^18.2.39",
        "@types/react-dom": "^18.2.17",
        "@types/react-router-dom": "^5.3.3",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-modal": "^3.11.2",
        "react-router-dom": "^6.20.0",
        "swr": "2.2.4",
        "zod": "3.23.8"
    },
    "devDependencies": {
        "@jest/environment": "^27.0.3",
        "@types/jest": "26.0.24",
        "esbuild": "*",
        "husky": "^8.0.0",
        "jest": "29.5.0",
        "npm-run-all": "*",
        "prettier": "^3.0.3",
        "pretty-quick": "^3.1.0",
        "ts-jest": "29.1.0",
        "typescript": "^4.3.3"
    },
    "scripts": {
        "clean": "rm -rf ./build/* && mkdir -p ./build",
        "watch": "npm-run-all clean --parallel watch:*",
        "watch:tsc": "tsc --watch",
        "watch:statics": "cp public/* build/",
        "watch:esbuild": "node esbuild.mjs --watch",
        "watch:server": "cd server && MOCK=true npm-run-all start",
        "build": "npm-run-all clean --parallel build:*",
        "build:statics": "cp public/* build/",
        "build:tsc": "tsc",
        "build:esbuild": "node esbuild.mjs",
        "test": "jest --config jest.config.js",
        "prepare": "husky install"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all",
            "ie 11"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version",
            "ie 11"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged --pattern \"**/*.*(js|json|jsx|ts|tsx|css|scss|less)\""
        }
    }
}
