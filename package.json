{
    "name": "permittering",
    "version": "0.1.0",
    "private": true,
    "homepage": "/permittering",
    "dependencies": {
        "@grafana/faro-react": "^2.2.3",
        "@grafana/faro-web-sdk": "^2.2.3",
        "@navikt/aksel-icons": "^6.17.0",
        "@navikt/ds-css": "^6.17.0",
        "@navikt/ds-react": "^6.17.0",
        "@navikt/fnrvalidator": "^1.3.13",
        "@navikt/nav-dekoratoren-moduler": "^3.6.2",
        "@navikt/virksomhetsvelger": "^0.0.17",
        "@types/react": "^18.3.28",
        "@types/react-dom": "^18.3.7",
        "@types/react-router-dom": "^5.3.3",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-modal": "^3.16.3",
        "react-router-dom": "^6.30.3",
        "swr": "^2.4.0",
        "zod": "^3.25.76"
    },
    "devDependencies": {
        "@jest/environment": "^30.2.0",
        "@types/jest": "^26.0.24",
        "esbuild": "^0.27.3",
        "husky": "^9.1.7",
        "jest": "^30.2.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^3.8.1",
        "pretty-quick": "^3.3.1",
        "ts-jest": "^29.4.6",
        "typescript": "^4.9.5"
    },
    "scripts": {
        "clean": "rm -rf ./build/* && mkdir -p ./build",
        "watch": "npm-run-all clean --parallel watch:*",
        "watch:tsc": "tsc --watch",
        "watch:statics": "cp public/* build/",
        "watch:esbuild": "node esbuild.mjs --watch",
        "watch:server": "cd server && MOCK=true npm-run-all start",
        "build": "npm-run-all clean --parallel build:*",
        "build:statics": "cp public/* build/",
        "build:tsc": "tsc",
        "build:esbuild": "node esbuild.mjs",
        "test": "jest --config jest.config.js",
        "prepare": "husky install"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all",
            "ie 11"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version",
            "ie 11"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged --pattern \"**/*.*(js|json|jsx|ts|tsx|css|scss|less)\""
        }
    }
}
